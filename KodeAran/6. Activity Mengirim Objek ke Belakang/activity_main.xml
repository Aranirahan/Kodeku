<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.ngoding.aran.activitymengirimobjekkebelakang.MainActivity">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text="Activity Penerima"/>
    <TextView
        android:id="@+id/tvKeterangan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(Belum Menerima Objek)"/>
    <Button
        android:id="@+id/btnPindahKeActivityPengirim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pindah ke activity pengirim"/>
    <TextView
        android:id="@+id/tvData"
        android:textSize="20dp"
        android:text="..."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"/>

</LinearLayout>